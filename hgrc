[alias]
aa = add
br = bookmark
ca = !hg add && hg commit $@
ci = commit
co = update -r $1
df = diff
lg = !hg log -f -l 10 --template '{rev}:{node|short} - {author|person}, {date|age}: {desc|firstline}\n' | sort -n
lgg = !hg log -f --template '{rev}:{node|short} - {author|person}, {date|age}: {desc|firstline}\n' | sort -n
mg = merge
re = rebase
sh = shelve
up = !hg pull --rebase
push = push --rev .
