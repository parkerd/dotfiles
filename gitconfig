fe = fetch
in = !git fetch && git lg master..origin/master
lg = log --pretty=format:'%h - %an, %ar: %s' --reverse -10
mg = merge
ms = merge --squash
out = !git fetch && git lg origin/master..master
re = rebase
st = status
up = pull
